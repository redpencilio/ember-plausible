{
  "private": true,
  "repository": "https://github.com/redpencilio/ember-plausible",
  "license": "MIT",
  "author": "redpencil.io",
  "scripts": {
    "build": "pnpm --filter ember-plausible build",
    "lint": "pnpm --filter '*' lint",
    "lint:fix": "pnpm --filter '*' lint:fix",
    "prepare": "pnpm build",
    "start": "concurrently 'pnpm:start:*' --restart-after 5000 --prefixColors auto",
    "start:addon": "pnpm --filter ember-plausible start --no-watch.clearScreen",
    "start:test-app": "pnpm --filter test-app start",
    "test": "pnpm --filter '*' test",
    "test:ember": "pnpm --filter '*' test:ember",
    "release": "pnpm --filter ember-plausible release"
  },
  "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
  "devDependencies": {
    "@glint/core": "^1.2.1",
    "concurrently": "^9.1.2",
    "prettier": "^3.0.3",
    "prettier-plugin-ember-template-tag": "^2.0.2"
  }
}
